/* Richtext: installation_richtext.css */
QMainWindow { background: #2d2d2d; }
QFileDialog { background: #2d2d2d; }
QDialog { background: #2d2d2d; }
YQWizard { background: #2d2d2d; }

#LogoHBox {
  border-image: none;
  padding: none;
  border: none;
  background-color: #999;
}

#work_area {
  border-image: none;
  padding: 1em;
  border: none;
  background-color: #2d2d2d;
}

YQLogView,
QTextEdit,
YQRichText > YQTextBrowser {
  qproperty-frameShape: NoShape;
  color: #eee;
  border-radius: 4px;
  border: 1px inset #161616;
  background-color: #262626;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}

QFrame { background-color: #2d2d2d; color: #eee; }

QLabel, YQDialog {
  color: #eee;
  background-color: #2d2d2d;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}

QHeaderView::section {
  background-color: #262626;
  color: #ccc;
  border: 1px outset #161616;
  border-radius: 2px;
  padding: 6px 8px;
}
QHeaderView::down-arrow { image: url(inst_arr_down.png); padding-top: 2px; }
QHeaderView::up-arrow { image: url(inst_arr_up.png); padding-top: 2px; }

QAbstractScrollArea { background-color: #262626; }

QTreeView, QTreeWidget {
  color: #eee;
  background-color: #262626;
  border: 1px inset #161616;
  alternate-background-color: #2a2a2a;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
  border-radius: 4px;
}

QTableWidget,
QTableView {
  color: #eee;
  background-color: #262626;
  border: 1px inset #161616;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
  border-radius: 4px;
}
YQTree, YQTable {
  color: #eee;
  background-color: #2d2d2d;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}

YQPakageSelector {
  color: #eee;
  background-color: #262626;
  border: 1px inset #161616;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
  border-radius: 4px; 
}

YQGenericDetailsView,
YQDescriptionDialog,
YQPkgDescriptionView { color: #eee; background-color: #262626; }

YQPkgPatternCategoryItem { background-color: #2d2d2d; color: #eee; }

QListWidget {
  color: #eee;
  background-color: #2d2d2d;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}

#DialogHeadingLeft {
  font-family: Raleway, Sans-serif;
  font: 24pt;
  color: #28ae73;
  margin-top: 100%;
  margin-right: 20px;
  qproperty-alignment:AlignRight;
}
#DialogHeadingTop {
  font-family: Raleway, Sans-serif;
  font: 24pt;
  color: #28ae73;
}

QSpinBox,
QDateEdit,
QTimeEdit { color: #eee; background-color: #262626; padding: 3px 8px; min-height:24px; selection-background-color: #28ae73; selection-color: #2d2d2d; }

QDateEdit::disabled, QTimeEdit::disabled{ color: gray; background-color: #2d2d2d; }

QRadioButton {color: #eee }

YQWidgetCaption  { color: #eee; background-color:#2d2d2d; }
YQWidgetCaption::disabled  {  color: gray; background-color:#2d2d2d; }

QLineEdit {
  color: #eee;
  background-color: #444;
  border-radius: 2px;
  padding: 3px 8px;
  min-height: 24px;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
  border: 1px inset #161616;
}

QCheckBox { min-height: 20px; color: #eee; }

QCheckBox::disabled { color: gray; background-color: #2d2d2d; }

YQCheckBoxFrame::indicator:checked,
QCheckBox::indicator:checked {
  image: url(inst_checkbox-on.png);
}
YQCheckBoxFrame::indicator:unchecked,
QCheckBox::indicator:unchecked {
  image: url(inst_checkbox-off.png);
}
YQCheckBoxFrame::indicator:disabled,
QCheckBox::indicator:disabled {
  opacity: 0.5;
}

QRadioButton::indicator {
   image: url(inst_radio-button-unchecked.png);
}
QRadioButton::indicator:checked {
   image: url(inst_radio-button-checked.png);
}

YQMultiLineEdit QTextEdit { color: #fff; }

QMessageBox { background-color: #2d2d2d; }

.conflict QWidget { color: #fff; }

YQMultiProgressMeter {
  background-color: transparent;
  border: 1px inset #161616;
  border-radius: 4px;
}

YQProgressBar > YQWidgetCaption { margin-top: 20px; }

/* Tab bars*/
QTabWidget::pane { border: none; }

QTabBar::tab {
  color: #bbb;
  background: #262626;
  border: 1px solid #161616;
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
  font-weight: bold;
  padding: 4px 8px;
  min-width: 180px;
  min-height: 24px;
}

QTabBar::tab:selected {
  color: white;
  background: #2d2d2d;
  border: 1px solid #161616; 
  border-bottom: none;
}

QTabBar::tab:!selected:hover {
  color: #28ae73;
}

QComboBox {
  border: 1px solid #656565;
  border-radius: 2px;
  color: #eeeeee;
  min-height: 24px;
  padding: 3px 12px;
  background: #262626;
  font-weight: bold;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}
QComboBox::disabled {
  color: gray;
  background-color: #2d2d2d
}
QComboBox:focus {
  border: 1px solid #28ae73;
  color: #28ae73;
}
/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
  color: #fff;
}
QComboBox::drop-down { /* arrow part of the widget */
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 24px;
  border: none;
}
QComboBox::down-arrow {
  image: url(inst_arr_down.png);
}
QComboBox::down-arrow:on { /* shift the arrow when popup is open */
  image: url(inst_arr_up.png);
  top: 0px;
  left: 0px;
}
QComboBox QAbstractItemView,
QMenuBar {
  background-color: #2d2d2d;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}

QMenuBar::item {
  color: #eee;
  spacing: 3px; /* spacing between menu bar items */
  padding: 6px 4px ;
  background: transparent;
  border-radius: 2px;
  margin: 6px 0px;
}

QMenuBar::item:selected { /* when selected using mouse or keyboard */
  color: #fff;
  background: #28ae73;
 }

QMenuBar::item:pressed {
  color: #28ae73;
  background: #2d2d2d;
}
QMenu {
  color: #eeeeee;
  background: #262626;
  border: 1px outset #161616;
  selection-background-color: #28ae73;
  selection-color: #2d2d2d;
}
QGroupBox {
  /* Eventually I'd like to have this global. Possibly belongs to style.qss */
  border: 0px;
  margin-top: 2.5ex;
  margin-left: 10px;
}
QGroupBox::title {
  subcontrol-origin: margin;
  subcontrol-position: left top;
  font-weight: bold;
  color: #eee;
}
QPushButton {
  border: 1px solid #175f48;
  border-radius: 2px;
  background-color: #2d2d2d;
  min-height: 24px;
  padding: 3px 12px;
  color: #28ae73;
  font-weight: bold;
}
QPushButton:focus, QPushButton:focus:default {
  border: 1px solid #28ae73;
}
QPushButton:focus:default {
  background-color: #28ae73;
}
QPushButton:disabled,
QPushButton:default:disabled {
  color: rgba(255,255,255,70);
  border: 1px solid rgba(255,255,255,30);
}
QPushButton:default:pressed, QPushButton:pressed {
  border: 1px solid #175f48;
  color: #2d2d2d;
  background-color: #175f48;
}
QPushButton:hover, QPushButton:default:hover  {
  background-color: #28ae73;
  color: #2d2d2d;
  border: 1px solid #28ae73;
}
QPushButton:default {
  border: 1px solid #28ae73;
  color: #2d2d2d;
  background-color: #28ae73;
}
/* odd dropdown button widget */
QPushButton::menu-indicator,
QPushButton::menu-indicator:pressed,
QPushButton::menu-indicator:open {
  subcontrol-origin: margin;
  left: -7px;
  subcontrol-position: right;
  image: url(inst_arr_down.png);
}
QPushButton:open { /* when the button has its menu open */
  color: #fff;
  background: #2d2d2d;
  border: 1px solid #161616;
}

/* Tab Buttons               */

QToolButton {
  border: 1px solid #161616;
  background-color: #262626;
}
QToolButton::left-arrow {
  image: url(inst_arr_left.png);
}
QToolButton::right-arrow {
  image: url(inst_arr_right.png);
}

/* Button Menus               */

QPushButton QMenu {
  border: 1px solid #161616;
}
QPushButton QMenu::item {
  background-color: #262626;
  color: #fff;
}
QPushButton QMenu::item:selected {
  background-color: #28ae73;
  color: white;
}

QProgressBar {
  border: 1px inset #161616;
  border-radius: 3px;
  text-align: center;
  color: #fff;
  background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #1b1b1b, stop: 1 #1f1f1f);
}
QProgressBar::chunk {
  background-color:  #28ae73;
  border: 1px solid #0cad7a;
  border-radius: 2px;
}

/* Vertical Scrollbars        */

QScrollBar:vertical {
  width: 14px;
  margin: 0px 0px 0px 0px;
  background-color: #222;
  border: 1px inset #161616;
}
QScrollBar::handle:vertical {
  min-height: 16px;
  border: 1px solid #161616;
  background-color: #aaa;
  border-radius: 2px;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  subcontrol-origin: margin;
  border: none;
  height: 0px;
  width: 0px;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  subcontrol-origin: margin;
  border: none;
  height: 0px;
  width: 0px;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
  width: 0px;
  height: 0px;
}
QScrollBar::up-arrow:vertical { background-image: none; }
QScrollBar::down-arrow:vertical {  background-image: none; }

/* Horizontal Scrollbars      */

QScrollBar:horizontal {
  height: 14px;
  margin: 0px 0px 0px 0px;
  background-color: #222;
  border: 1px inset #161616;
}
QScrollBar::handle:horizontal {
  min-width: 16px;
  border: 1px solid #161616;
  background-color: #aaa;
  border-radius: 2px;
}
QScrollBar::add-line:horizontal {
  border: none;
  background-color: transparent;
  height: 0px;
  width: 0px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
  border: none;
  background-color: transparent;
  height: 0px;
  width: 0px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
  width: 0px;
}
QScrollBar::right-arrow:horizontal { background-image: none; }
QScrollBar::left-arrow:horizontal { background-image: none; }

BusyBar {
 qproperty-frameShape: NoShape;
 border-width: 3px;
 border: 1px solid #28ae73;
 border-radius: 4px;
}
QToolTip {
  background-color: #fffbdd;
}
QSplitter::handle {
  image: url(separator.png);
  height: 10px;
}

QGroupBox {
  border: 1px flat #656565;
  border-radius: 4px;
  padding: 2px;
}

#RepoUpgradeLabel {
  color: #fff;
  background: #28ae73;
} 

YQPkgVersionsView QWidget { background: #2d2d2d; }

QY2HelpDialog
{
   qproperty-searchResultBackground: #666;
   qproperty-searchResultForeground: #fff;
}
YQBarGraph {
   qproperty-BackgroundColors: #36638c, #73b8e7, #3b73ff;
   qproperty-ForegroundColors: #fff; #fff, #fff;
}
